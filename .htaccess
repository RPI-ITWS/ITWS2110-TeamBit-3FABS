RewriteEngine On
Redirect /browse /browse/new
RewriteRule ^</p>'browse/(new|top)$ /browse.php?sort=$1 [QSA]
RewriteRule ^</p>'browse/(new|top)/([0-9]+)$ /browse.php?sort=$1&page=$2 [QSA]
RewriteRule ^</p>'browse/friend/(new|top)$ /browse.php?sort=$1&friend=1 [QSA]
RewriteRule ^</p>'browse/friend/(new|top)/([0-9]+)$ /browse.php?sort=$1&page=$2&friend=1 [QSA]
RewriteRule ^</p>'login$ /login.php [QSA]