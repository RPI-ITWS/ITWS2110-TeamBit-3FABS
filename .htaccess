RewriteEngine On
RewriteBase /project/
RewriteRule ^browse$ /browse/new [R=302, L]
RewriteRule ^browse/(new|top)$ /browse.php?sort=$1 [QSA]
RewriteRule ^browse/(new|top)/([0-9]+)$ /browse.php?sort=$1&page=$2 [QSA]
RewriteRule ^browse/friend/(new|top)$ /browse.php?sort=$1&friend=1 [QSA]
RewriteRule ^browse/friend/(new|top)/([0-9]+)$ /browse.php?sort=$1&page=$2&friend=1 [QSA]
RewriteRule ^login$ /login.php [QSA]